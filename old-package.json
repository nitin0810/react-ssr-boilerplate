{
    "name": "react-ssr-boilerplate",
    "version": "5.1.0",
    "private": true,
    "engines": {
      "node": ">=10.15"
    },
    "scripts": {
      "start": "node scripts/start.js",
      "build": "node scripts/build.js",
      "test": "node scripts/test.js",
      "start:prod": "node scripts/startProd.js",
      "lint": "eslint src/**/*.js"
    },
    "husky": {
      "hooks": {
        "pre-commit": "lint-staged"
      }
    },
    "lint-staged": {
      "*.js": [
        "npm run lint"
      ],
      "*.{js,json,css,md}": [
        "npm run format",
        "git add"
      ]
    },
    "dependencies": {
      "@testing-library/react": "^9.1.4",
      "body-parser": "^1.19.0",
      "jest": "^24.9.0",
      "prettier": "^1.18.2",
      "response-time": "^2.3.2"
    }
  }
  